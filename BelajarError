public class Proyek {
 public static void main(String[] args) {
       
            System.out.println(pembagian(10, 2));
        
          
 }
    

    public static String pembagian(int x, int y) {
        try {
            double hasil = x / y;
            return "sukses";
        } catch (ArithmeticException e) {
            return e.getMessage();
        }
    }
}

